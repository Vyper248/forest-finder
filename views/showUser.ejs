<% include partials/header %>

<div class="container">
    <div class="jumbotron">
        <h1>Your Forests</h1>
    </div>
</div>

<div class="container">
	<div class="row">
	<% for (var i = 0; i < forests.length; i++) { %>
		<div class="col-md-6 col-lg-4">
			<a href="/forests/<%= forests[i]._id %>">
				<div class="thumbnail">
					<h4 class="forestName"><%= forests[i].name %></h4>
					<div style="background-image:url('<%= forests[i].image %>');" class="image"></div>
				</div>
			</a>
		</div>
	<% } %>
	</div>
</div>

<% include partials/footer %>
